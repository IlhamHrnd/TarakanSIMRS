@using TarakanSIMRS.Areas.Tarakan.Models.Registration
@model RegistrationReferViewModel

@if (!string.IsNullOrEmpty(Model.fromRegistrationNo))
{
    <div class="card">
        <div class="card-header">Refer From Unit</div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered">
                    <tbody>
                        <tr>
                            <th scope="row">Service Unit</th>
                            <td>@Model.serviceUnitName</td>
                        </tr>
                        <tr>
                            <th scope="row">Refer From</th>
                            <td>@Model.fromRegistrationNo</td>
                        </tr>
                        <tr>
                            <th scope="row">Physician</th>
                            <td>@Model.paramedicName</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
}