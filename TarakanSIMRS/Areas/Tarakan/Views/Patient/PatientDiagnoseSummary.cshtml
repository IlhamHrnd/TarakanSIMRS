@if (!string.IsNullOrEmpty(Model.getPatientDiagnoses))
{
    <div class="card">
        <h3 class="mb-1" style="margin: 2px">Diagnosis :</h3>
        <div class="m-0" style="margin: 2px">@Html.Raw(Model.getPatientDiagnoses)</div>
